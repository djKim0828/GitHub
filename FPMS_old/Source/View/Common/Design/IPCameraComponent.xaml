<UserControl x:Class="EmWorks.View.IPCameraComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:EmWorks.View"
             mc:Ignorable="d"
             Tag="1"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid  Background="{DynamicResource colorBgDialog}">
        <Grid.RowDefinitions>
            <RowDefinition Height="24" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <TextBlock x:Name="txtBlock" Style="{DynamicResource txbBlack}" />
        </Grid>
        <Grid Grid.Row="1">
            <Button x:Name="Clear" Content="Clear" Style="{DynamicResource btnNormal}"></Button>
        </Grid>
    </Grid>
</UserControl>
