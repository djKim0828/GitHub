<UserControl x:Class="EmWorks.View.TK4S"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:EmWorks.View"
             mc:Ignorable="d"
             Tag="1" d:DesignWidth="500" Height="800">
    <Grid  Background="{DynamicResource colorBgDialog}">
        <Grid.RowDefinitions>
            <RowDefinition Height="24" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid x:Name="grdTitleTK4S" Grid.Row="0" Background="Black">
            <Label x:Name="lblTK4S" Content="TK4S" Style="{DynamicResource lblInformationTitle}" />
        </Grid>
    </Grid>
</UserControl>