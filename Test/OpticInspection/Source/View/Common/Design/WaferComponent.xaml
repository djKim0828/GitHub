<UserControl x:Class="EmWorks.View.WaferComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EmWorks.View"
             mc:Ignorable="d" 
             Tag="1"
             d:DesignHeight="640" d:DesignWidth="640">
    <Grid x:Name="grdMain" Width="640" Height="640" HorizontalAlignment="Center" >
        <Grid x:Name="grdCanvas" HorizontalAlignment="Center">
            <Path x:Name="imgEllipse" Data="{DynamicResource icoEllipse}" Fill="White" Stretch="Fill" />
            <Grid x:Name="grdButtons" HorizontalAlignment="Center">
            </Grid>
        </Grid>
    </Grid>
</UserControl>
